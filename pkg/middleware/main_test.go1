package middleware

import (
	"fmt"
	"net/http"
	"os"
	"testing"

	httpServer "github.com/RomaBiliak/lets-go-chat/pkg/http"
)



func TestMain(m *testing.M) {
	mux := http.NewServeMux()


	mux.Handle("/test-log-request", LogRequest(func(w http.ResponseWriter, r *http.Request){
		fmt.Println("teeeeest")
	}))


	httpServer.Start(":8080", mux)

	os.Exit(m.Run())
}
